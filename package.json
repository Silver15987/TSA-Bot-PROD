{
  "name": "discord-vc-faction-bot",
  "version": "1.0.0",
  "description": "Discord bot with VC tracking, faction management, and economy system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "refresh-commands": "tsx scripts/refresh-commands.ts",
    "set-faction-category": "tsx scripts/set-faction-category.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "voice-tracking",
    "faction",
    "economy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@azure/identity": "^4.5.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "mongodb": "^6.3.0",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "winston": "^3.11.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
